#include<iostream>
#include<queue>
int main()
{
priority_queue<int> pq;//it creates a max heap
pq.push(10);
pq.push(15);
pq.push(20);
pq.top();//it gives the current maximum element in heap // o/p: 20
      20
      / \
     10  15

priority_queue<int,vector<int>,greater<int>> pq; //creating min heap 
pq.push(10);
pq.push(15);
pq.push(20);
pq.top();//gives curr minimum element in heap // o/p : 10
     10
     / \
    15  20
